    "class Location:\n",
    "    def __init__(self, name, message, items, npcs):\n",
    "        self.name = name\n",
    "        self.message = message\n",
    "        self.items = items\n",
    "        self.npc = npcs\n",
    "\n",
    "l1 = Location('tavern', 'You arrive at the tavern', [], [])\n",
    "l2 = Location('river', 'A large river, too ferocious to pass, rages on to the north of you. No living creature could survive currents like those.', [], [])\n",
    "l3 = Location('forest', 'A dense forest stretches on to the south.', [], [])\n",
    "l4 = Location('canyon', 'A canyon lays to the west of you, too steep to climb down and too wide to cross.', [], [])\n",
    "l5 = Location('river/mountain', 'To the north you hear river rapids and your east you see tall snowy mountains.', [], [])\n",
    "l6 = Location('river/canyon', 'To the north a river rages on and to the west you see a canyon.', [], [])\n",
    "l7 = Location('canyon/forest',\"A canyon lays to the west and a great forest to the south.\", [], [])\n",
    "l8 = Location('forest/mountains', \"You feel the chill of snowy mountains to the east and hear the sounds of a forest to the south.\", [], [])\n",
    "l9 = Location('mountains', \"Tall mountains to the east tower above you.\", [], [])\n",
    "\n",
    "print(l1.message)\n",
    "\n",
    "def game():\n",
    "    x = 0.0\n",
    "    y = 0.0\n",
    "    locations = {\n",
    "        (0.0,0.0) : l1, \n",
    "        (0.0,1.0) : l2, \n",
    "        (0.0,-1.0) : l3, \n",
    "        (-1.0,0.0) : l4, \n",
    "        (1.0,1.0) : l5, \n",
    "        (-1.0,1.0) : l6, \n",
    "        (-1.0, -1.0) : l7, \n",
    "        (1.0,-1.0) : l8,\n",
    "        (1.0,0.0) : l9}\n",
    "    \n",
    "    max_coord_pos = 1.0\n",
    "    max_coord_neg = -1.0\n",
    "\n",
    "    print(\"After talking to the tavernkeep you decide to explore. Press 'i' to recieve further instruction.\")\n",
    "    command = input().lower()\n",
    "    while command:\n",
    "        illegal = False\n",
    "        if command == 'go north':\n",
    "            if y + 1 > max_coord_pos:\n",
    "                illegal = True\n",
    "                print(\"The river is too fast to pass through.\") \n",
    "            else:\n",
    "                y += 1.0\n",
    "        elif command == 'go east':\n",
    "            if x + 1 > max_coord_pos:\n",
    "                illegal = True\n",
    "                print(\"The mountains are too steep to climb.\")\n",
    "            else:\n",
    "                x += 1.0\n",
    "        elif command == 'go south':\n",
    "            if y - 1 < max_coord_neg:\n",
    "                illegal = True\n",
    "                print(\"The forest is too overgrown to pass through.\")\n",
    "            else:\n",
    "                y -= 1.0\n",
    "        elif command == 'i':\n",
    "            illegal = True\n",
    "            print(\"Welcome to the test map! Use commands 'go north', 'go east', 'go south', and 'go west' to navigate the world. You can always press 'i' to get a refresher on these instructions. Have fun!\")\n",
    "        elif command == 'go west':\n",
    "            if x - 1 < max_coord_neg:\n",
    "                illegal = True\n",
    "                print(\"The canyon is impassable.\")\n",
    "            else:\n",
    "                x -= 1.0\n",
    "\n",
    "        if not illegal:\n",
    "            print(locations[(x,y)].message)\n",
    "\n",
    "        command = input().lower()\n",
    "        \n",
    "        \n",
    "        \n",
    "        \n",
    "game()"
